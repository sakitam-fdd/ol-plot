{
  "name": "ol-plot-vue",
  "version": "1.0.0",
  "type": "module",
  "description": "openlayers 标绘 Vue 组件",
  "main": "dist/ol-plot-vue.js",
  "module": "dist/ol-plot-vue.es.js",
  "unpkg": "dist/ol-plot-vue.js",
  "commonjs": "dist/ol-plot-vue.cjs.js",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/ol-plot-vue.es.js",
      "require": "./dist/ol-plot-vue.cjs.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "keywords": [
    "javascript",
    "webgis",
    "canvas",
    "ol",
    "openlayers",
    "map",
    "plot",
    "map",
    "vis",
    "gis",
    "vue"
  ],
  "author": "sakitam-fdd <smilefdd@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sakitam-fdd/ol-plot",
  "private": false,
  "scripts": {
    "dev": "vite build --mode watch",
    "build": "pnpm run type-check && pnpm run build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "^0.14.0",
    "@vueuse/core": "^10.0.2",
    "ol-plot": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/tsconfig": "^0.1.3",
    "vite": "^4.3.1",
    "vue-tsc": "^1.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
